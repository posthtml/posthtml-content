{
  "name": "posthtml-content",
  "description": "Content Plugin for PostHTML",
  "version": "1.0.0",
  "main": "index.js",
  "engines": { "node": ">=4" },
  "scripts": {
    "lint": "standard",
    "test": "nyc ava test/index.js",
    "logs": "standard-changelog > CHANGELOG.md",
    "docs": "jsdoc2md index.js > INDEX.md",
    "clean": "rm -rf .nyc_output coverage jsdoc-api dmd",
    "start": "sudo npm run clean && npm run lint && sudo npm test"
  },
  "dependencies": {
    "read-cache": "^1.0.0"
  },
  "devDependencies": {
    "ava": "0.17.x",
    "coveralls": "2.x",
    "nyc": "10.x",
    "postcss": "^5.x",
    "postcss-nested": "^1.x",
    "posthtml": "^0.9.x",
    "standard": "8.x"
  },
  "ava": {
    "verbose": "true"
  },
  "files": [
    "lib",
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "content",
    "posthtml",
    "posthtmlplugin"
  ],
  "author": {
    "name": "Michael Ciniawsky",
    "email": "michael.ciniawsky@gmail.com"
  },
  "contributors": [
    {
      "name": "Jeff Escalante",
      "email": "gh.je@mailhero.io"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/posthtml/posthtml-content.git"
  },
  "homepage": "https://github.com/posthtml/posthtml-content",
  "bugs": "https://github.com/posthtml/posthtml-content/issues",
  "license": "MIT"
}
